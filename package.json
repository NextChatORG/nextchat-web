{
  "name": "nextchat-web",
  "version": "0.1.0",
  "license": "GPL-2.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "format": "prettier **/*.{vue,ts,json,html} --write --ignore-path .gitignore",
    "format:check": "prettier **/*.{vue,ts,json,html} --check --ignore-path .gitignore",
    "postinstall": "husky install"
  },
  "dependencies": {
    "vue": "^3.1.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.1.1",
    "husky": "^6.0.0",
    "prettier": "^2.3.1",
    "typescript": "^4.3.2",
    "vite": "^2.3.7",
    "vue-tsc": "^0.1.7"
  }
}
