{
  "name": "nextchat-web",
  "version": "1.0.0-alpha",
  "scripts": {
    "dev": "vite-ssr dev",
    "build": "vue-tsc --noEmit && vite-ssr build",
    "serve": "vite preview",
    "lint": "eslint --ignore-path .gitignore --ext vue,ts,js .",
    "lint:fix": "eslint --fix --ignore-path .gitignore --ext vue,ts,js .",
    "format": "prettier --ignore-path .gitignore --write {*,**/*,**/**/*}.{ts,md,json} {**/*,**/**/*}.vue",
    "format:check": "prettier --ignore-path .gitignore --check {*,**/*,**/**/*}.{ts,md,json} {**/*,**/**/*}.vue"
  },
  "dependencies": {
    "@vueuse/head": "^0.7.2",
    "vite-ssr": "^0.14.3",
    "vue": "3"
  },
  "devDependencies": {
    "@types/node": "^16.11.11",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "@vitejs/plugin-vue": "^1.9.3",
    "autoprefixer": "^10.4.0",
    "cssnano": "^5.0.12",
    "cssnano-preset-advanced": "^5.1.8",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "postcss": "^8.4.4",
    "prettier": "^2.4.1",
    "sass": "^1.44.0",
    "typescript": "^4.4.3",
    "vite": "^2.6.4",
    "vue-eslint-parser": "^8.0.1",
    "vue-router": "4",
    "vue-tsc": "^0.3.0"
  }
}
